ZAI_API_KEY=your-zai-api-key
LITELLM_MASTER_KEY=sk-litellm-master-key
ANTHROPIC_API_KEY=sk-litellm-master-key
ANTHROPIC_BASE_URL=http://localhost:4000
API_TIMEOUT_MS=3000000

# GLM model mappings for Claude SDK aliases
ANTHROPIC_DEFAULT_OPUS_MODEL=glm-5
ANTHROPIC_DEFAULT_SONNET_MODEL=glm-5
ANTHROPIC_DEFAULT_HAIKU_MODEL=glm-4.5-air

# Pipeline iteration limits
# MAX_REVIEW_ITERATIONS: Maximum number of review-fix cycles before proceeding to report
MAX_REVIEW_ITERATIONS=3

# MAX_GREEN_FIX_ATTEMPTS: Maximum attempts to fix failing tests after initial GREEN stage
MAX_GREEN_FIX_ATTEMPTS=3

# MAX_SECURITY_ITERATIONS: Maximum number of security-review/fix cycles before proceeding to report
MAX_SECURITY_ITERATIONS=2

# SECURITY_MODEL: Model override for the security review agent (defaults to PIPELINE_MODEL)
# SECURITY_MODEL=sonnet
